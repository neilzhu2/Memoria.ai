  "devDependencies": {
    "@types/react": "~19.1.0",
    "typescript": "~5.9.2",
    "@testing-library/react-native": "^12.3.0",
    "@testing-library/jest-native": "^5.4.3",
    "jest": "^29.7.0",
    "react-test-renderer": "19.1.0",
    "@axe-core/react-native": "^4.8.0",
    "react-native-accessibility-engine": "^1.0.0",
    "detox": "^20.13.0",
    "jest-circus": "^29.7.0"
  },
  "private": true
}
