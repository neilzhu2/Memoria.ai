{
  "name": "memoria-mobile",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:accessibility": "jest --testNamePattern=accessibility",
    "test:elderly-user": "jest --testNamePattern=elderly",
    "test:e2e": "detox test",
    "test:e2e:ios": "detox test --configuration ios.sim.debug",
    "test:e2e:android": "detox test --configuration android.emu.debug",
    "accessibility:audit": "node scripts/accessibility-audit.js",
    "accessibility:report": "node scripts/generate-accessibility-report.js"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.8",
    "@tanstack/react-query": "^5.89.0",
    "expo": "~54.0.9",
    "expo-av": "~16.0.7",
    "expo-haptics": "~15.0.7",
    "expo-media-library": "~18.2.0",
    "expo-secure-store": "~15.0.7",
    "expo-speech": "~14.0.7",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-mmkv": "^3.3.3",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "zustand": "^5.0.8"
